{
  "name": "my-remix-app",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "homepage": "https://my-remix-app.kaibun.net/",
  "description": "A Remix app deployed on Kubernetes.",
  "author": {
    "name": "Jean-Denis Vauguet",
    "email": "contact@kaibun.net",
    "url": "https://kaibun.net/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaibun/my-remix-app-on-kubernetes.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/express": "^1.13.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4"
  }
}
